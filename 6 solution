
#include <iostream>
#include <vector>
using namespace std;
int main()
{
    int n, s; // n - размер вектора, s - искомая сумма
    // продвинутое решение через остатки от деления (вспомнил)
    cin >> n >> s;
    vector<int> v(n, 0);
    vector<int> m(n, 0);
    for(int i = 0; i < n; i++)
    {
        cin >> v[i];  
        if(i == 0)
        {
            m[i] = v[i];   
        }
        else
        {
            m[i] = m[i - 1] + v[i];
        }
    }
    int cnt = 0;
    for(int i = 0; i < n; i++)
    {
        if(m[i] == s)
        {
            cnt++;
        }
        for(int j = 0; j < i; j++)
        {
            if(m[i] - m[j] == s)
            {
                cnt++;   
            }
        }
    }
    cout << cnt;
}
